@prefix log: <http://www.w3.org/2000/10/swap/log#>.
@prefix : <http://example.org/ns#>.

# beetle is a car
:beetle a :Car.
() log:onNegativeSurface {
:beetle :is :beautiful.

}

# everything is beautiful
(_:A) log:onNegativeSurface {
    () log:onNegativeSurface {
        _:A :is :beautiful.
    }.
}.

# query
(_:bd) log:onQuerySurface {
    :beetle :is :beautiful.
}.
