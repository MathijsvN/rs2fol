(_:A) log:onNegativeSurface {
   (_:l _:l1) log:onNegativeSurface {
      _:l r:first _:A.
      _:l r:rest _:l1.
      _:l1 r:first 0.
      _:l1 r:rest r:nil.
      _:l :add _:A.
   }.
}.

