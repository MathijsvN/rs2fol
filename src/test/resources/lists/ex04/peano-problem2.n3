(_:A) log:onNegativeSurface {
   (_:BN1 _:BN2) log:onNegativeSurface {
      _:BN1 r:first _:A.
      _:BN1 r:rest _:BN2.
      _:BN2 r:first 0.
      _:BN2 r:rest r:nil.
      _:BN1 :add _:A.
   }.
}.

(_:l01 _:l02 _:l011 _:l012) log:onQuerySurface {
      _:l01 r:first _:l011; r:rest _:l02 .
      _:l02 r:first 0; r:rest r:nil .
      _:l01 :add 0 .
}.