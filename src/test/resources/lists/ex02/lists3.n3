(_:first _:rest) log:onNegativeSurface {
	(_:A) log:onNegativeSurface {
		_:A r:first _:first.
		_:A r:rest _:rest.
	}.
}.

(_:A _:B) log:onQuerySurface {
    _:A r:first _:B.
    _:A r:rest :c.
}.